{
  "name": "lockdown",
  "version": "2.0.0",
  "description": "Zombie survival game built with Phaser, React, and Node/Express",
  "private": true,
  "main": "src/index.js",
  "proxy": "http://localhost:3001",
  "scripts": {
    "start": "nodemon ./bin/www",
    "build": "webpack --config webpack.prod.js",
    "devClient": "webpack serve --config webpack.dev.js --open",
    "db:reset": "node ./db/reset.js",
    "heroku-postbuild": "npm run build && npm start"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "pg": "^8.6.0",
    "pg-native": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "dotenv": "^10.0.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "optimize-css-assets-webpack-plugin": "^6.0.0",
    "sass-loader": "^11.0.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^3.0.0"
